{
  "name": "openapi-enforcer-lambda",
  "version": "0.0.1",
  "description": "OpenAPI Enforcer library for simplifying AWS Lambdas that handle HTTP requests.",
  "main": "src/index.ts",
  "dependencies": {
    "coverage": "^0.4.1"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.83",
    "@types/chai": "^4.2.21",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.7.2",
    "chai": "^4.3.4",
    "mocha": "^9.1.1",
    "openapi-enforcer": "^1.15.3",
    "ts-jest": "^27.0.5",
    "ts-node": "^10.2.1",
    "ts-standard": "^10.0.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "openapi-enforcer": "^1.15.3"
  },
  "scripts": {
    "build": "tsc -b tsconfig.json",
    "test": "set TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha src/test/enforcer.test.ts",
    "test:debug": "mocha --inspect-brk src/test/enforcer.test.ts",
    "test:dev": "npm run build && npm run test",
    "test:dev:debug": "npm run build && npm run test:debug",
    "lint": "ts-standard",
    "lint:fix": "ts-standard --fix",
    "coverage": "npm run test -- --coverage || exit 0"
  },
  "keywords": [
    "openapi-enforcer",
    "aws",
    "lambda"
  ],
  "author": "James Speirs <james.speirs@gmail.com>",
  "license": "Apache-2.0"
}
